{
  "name": "@one-for-all/page-engine",
  "version": "0.10.0",
  "description": "A qxp page engine",
  "license": "MIT",
  "author": "sunnyw <sunnywang@yunify.com>",
  "type": "module",
  "main": "dist/index.js",
  "module": "lib",
  "types": "lib/src/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "yarn run clean && yarn run tsc",
    "tsc": "tsc --build tsconfig.json",
    "bundle": "yarn run build && rollup -c",
    "clean": "rm -rf dist lib",
    "dev": "webpack serve --progress",
    "copy:assets": "mkdir -p ../../dist/images && cp -R ../ui/{assets,dist}/images/* ../../dist/images",
    "lint": "eslint src/**/*.ts --fix",
    "pack": "yarn pack",
    "test": "jest",
    "start": "run-p server watch",
    "watch": "webpack --watch",
    "server": "http-server --cors='*'",
    "type-check": "tsc --noEmit --skipLibCheck --pretty -w"
  },
  "dependencies": {
    "@codemirror/lang-javascript": "^0.19.3",
    "@one-for-all/render-engine": "0.18.0",
    "@one-for-all/schema-spec": "0.15.0",
