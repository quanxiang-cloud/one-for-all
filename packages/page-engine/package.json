    "@uiw/react-codemirror": "~4.2.0",
    "classnames": "^2.3.1",
    "lodash": "^4.17.21",
    "mobx": "^6.3.8",
    "mobx-react": "^7.2.1",
    "nanoid": "^3.1.30",
    "react": "^17.0.2",
    "react-dnd": "^14.0.4",
    "react-dnd-html5-backend": "^14.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.20.4",
    "react-use": "^17.3.2",
    "mitt": "~3.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.8",
