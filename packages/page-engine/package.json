    "sass": "^1.44.0",
    "sass-loader": "~12.4.0",
    "style-loader": "~3.3.1",
    "tailwindcss": "^2.2.19",
    "ts-loader": "~9.2.6",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "tslib": "2.3.1",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "~4.7.4",
    "webpack-notifier": "^1.14.1",
    "eslint": "8.7.0",
    "@rushstack/eslint-config": "2.5.1",
    "typescript": "4.5.5"
