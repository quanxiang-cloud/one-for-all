{
  "name": "@one-for-all/ui",
  "version": "0.3.3",
  "description": "Migration of qxp-web base components",
  "homepage": "",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "lib",
  "types": "lib/src/index.d.ts",
  "files": [
    "dist",
    "lib"
  ],
  "scripts": {
    "build": "yarn run clean && tsc --build tsconfig.json && yarn run build:icons && yarn run copy:assets",
    "tsc": "tsc --build tsconfig.json",
    "build:icons": "node scripts/build-icons.js",
    "bundle": "yarn run build && rollup -c",
    "copy:assets": "mkdir -p ../../dist/images && cp -R dist/images/* ../../dist/images",
    "pack": "yarn pack",
    "bundle:watch": "rollup -c -w",
    "clean": "rm -rf dist lib",
    "lint": "eslint src/**/*.ts --fix",
    "preview:icons": "node scripts/preview-icons.js",
    "test": "jest"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.0",
    "classnames": "^2.3.1",
    "lodash": "^4.17.21",
    "nanoid": "^3.1.30",
    "react": "^17.0.2",
    "react-color": "^2.19.3",
    "react-dom": "^17.0.2",
    "react-is": ">= 16.8.0",
    "react-popper": "^2.2.5",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@rollup/plugin-replace": "^4.0.0",
    "@types/react": "~17.0.38",
    "@types/react-color": "3.0.6",
    "@types/react-dom": "^17.0.11",
    "@types/styled-components": "^5.1.16",
    "autoprefixer": "10.4.2",
    "postcss": "8.4.5",
    "postcss-import": "^14.0.2",
    "rollup": "2.66.0",
    "rollup-plugin-esbuild": "^4.6.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-styles": "^3.14.1",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "1.51.0",
    "svg-spreact": "1.3.2",
    "tailwindcss": "^2.2.19",
    "tslib": "2.3.1",
    "mkdirp": "~1.0.4",
    "glob": "~7.2.0",
    "@types/lodash": "^4.14.178"
  }
}
